{
  "name": "mobilyflow-react-native-sdk-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator \"iPhone 16\"",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --scheme MobilyflowReactNativeSdkExample --mode Debug --extra-params \"-sdk iphonesimulator CC=clang CPLUSPLUS=clang++ LD=clang LDPLUSPLUS=clang++ GCC_OPTIMIZATION_LEVEL=0 GCC_PRECOMPILE_PREFIX_HEADER=YES ASSETCATALOG_COMPILER_OPTIMIZATION=time DEBUG_INFORMATION_FORMAT=dwarf COMPILER_INDEX_STORE_ENABLE=NO\""
  },
  "dependencies": {
    "@react-native-vector-icons/material-design-icons": "^12.3.0",
    "@react-native-vector-icons/material-icons": "^12.3.0",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@react-stateless-dialog/core": "^0.4.0-beta5",
    "@react-stateless-dialog/native": "^0.4.0-beta5",
    "@tanstack/react-query": "^5.90.2",
    "luxon": "^3.7.2",
    "react": "19.0.0",
    "react-native": "0.78.1",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-mmkv": "^3.3.3",
    "react-native-nitro-modules": "^0.29.6",
    "react-native-reanimated": "^4.1.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-unistyles": "^3.0.13",
    "react-native-worklets": "^0.6.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.28.4",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.78.0",
    "@react-native/metro-config": "0.78.0",
    "@react-native/typescript-config": "0.78.0",
    "@types/luxon": "^3.7.1",
    "react-native-builder-bob": "^0.36.0"
  },
  "engines": {
    "node": ">=18"
  }
}
